{
	"version": 2,
	"installCommand": "npm config set legacy-peer-deps true && npm i",
	"builds": [
		{
			"config": {
				"installCommand": "npm i --legacy-peer-deps --force"
			},
			"src": "src/main.ts",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "src/main.ts",
			"methods": [
				"GET",
				"POST",
				"PUT",
				"DELETE"
			]
		}
	]
}