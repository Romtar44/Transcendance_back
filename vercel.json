{
	"version": 2,
	"installCommand": "npm i  --legacy-peer-deps ",
	"builds": [
		{
			"installCommand": "npm i --legacy-peer-deps --force",
			"src": "src/main.ts",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "src/main.ts",
			"methods": [
				"GET",
				"POST",
				"PUT",
				"DELETE"
			]
		}
	]
}