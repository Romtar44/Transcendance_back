{
	"version": 2,
	"installCommand": "npm i  --legacy-peer-deps ",
	"builds": [
		{
			"config": {
				"installCommand": "npm i --legacy-peer-deps --force",
				"buildCommand": "npx prisma generate && npm run vercel-build"
			},
			"src": "src/main.ts",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "src/main.ts",
			"methods": [
				"GET",
				"POST",
				"PUT",
				"DELETE"
			]
		}
	]
}