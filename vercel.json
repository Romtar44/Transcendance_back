{
	"version": 2,
	"installCommand": "npm config set legacy-peer-deps true && npm i",
	"builds": [
		{
			"src": "src/main.ts",
			"use": "@vercel/node --legacy-peer-deps"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "src/main.ts",
			"methods": [
				"GET",
				"POST",
				"PUT",
				"DELETE"
			]
		}
	]
}